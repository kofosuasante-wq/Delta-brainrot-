local loadstring = loadstring -- store loadstring for later use

local function createUI()  
local screenGui = Instance.new("ScreenGui")  
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")  
screenGui.Name = "InstantStealUI"

local frame = Instance.new("Frame")  
frame.Parent = screenGui  
frame.Size = UDim2.new(0, 150, 0, 50)  
frame.Position = UDim2.new(0.8, 0, 0.8, 0)  
frame.BackgroundColor3 = Color3.new(0, 0, 0)  
frame.BackgroundTransparency = 0.5

local button = Instance.new("TextButton")  
button.Parent = frame  
button.Size = UDim2.new(0, 140, 0, 40)  
button.Position = UDim2.new(5, 0, 10, 0)  
button.Text = "Instant Steal"  
button.BackgroundColor3 = Color3.new(0, 1, 0)  
button.BackgroundTransparency = 0  
button.TextTransparency = 0

local function instantSteal()  
local player = game.Players.LocalPlayer  
local character = player.Character  
if not player or not character then return end

local humanoid = character:FindFirstChild("Humanoid")  
if not humanoid then return end

-- check if the player has brainrot  
local brainrotItem = character:FindFirstChild("Brainrot") -- assuming the brainrot item is a part named "Brainrot" inside the character

if not brainrotItem then  
print("you do not have brainrot")  
return  
end

-- teleport the player to their base (replace with your base's coordinates)  
local basePosition = vector3.new(0, 0, 0) -- replace with your base's coordinates  
character:MoveTo(basePosition)

-- remove the brainrot item from the player  
brainrotItem:Destroy()

print("brainrot stolen and teleported")  
end

button.MouseButton1Click:Connect(instantSteal)  
end

-- call the function to create the ui  
createUI()

-- loadstring implementation (this part might need adjustments depending on the executor)  
local script = loadstring(game.Workspace.RawAsset.Script)  
coroutine.wrap(script)()  
